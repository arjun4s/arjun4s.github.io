---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Blog Arjun4s</title>
	<link href="http://arjun4s.github.io/blog/atom.xml" rel="self"/>
	<link href="http://arjun4s.github.io/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://arjun4s.github.io/blog</id>
	<author>
		<name>Arjun4s</name>
	</author>
	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://arjun4s.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://arjun4s.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}
</feed>